{
  "name": "pltsvc-app",
  "description": "B.C. Government's Platform Product Registry - App",
  "version": "0.77.0",
  "private": true,
  "scripts": {
    "dev": "npx prisma generate && next dev --turbopack",
    "build": "next build",
    "start": "dotenv -e .env.test next start",
    "postinstall": "pnpm prisma-generate",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "dotenv -e .env.test -- prisma db push && NODE_OPTIONS='--experimental-vm-modules' jest --runInBand",
    "prisma-generate": "prisma generate",
    "prisma-push": "dotenv -e .env.local -- prisma db push",
    "migrate-data": "cat .env.local | grep DATABASE_URL > ../data-migrations/.env && cd ../data-migrations && npm install && npx migrate-mongo up",
    "email": "dotenv -e .env.local -- email dev --dir emails --port 3001",
    "email-build": "email build --packageManager=pnpm",
    "email-start": "dotenv -e .env.local -- email start --port 3001",
    "email-export": "email export --pretty --outDir emails-export --dir emails"
  },
  "engines": {
    "node": ">=22.1.0"
  },
